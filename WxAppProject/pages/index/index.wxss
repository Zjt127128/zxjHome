/**index.wxss**/
.page-body {
  margin: 0;
  padding: 0;
  height: 100%;
}
.transition {
  transition: all 5s ease;
}
.swiper {
  height: 350rpx;
  width: 100%;
  position: relative;
}

@media (min-width: 768px) { 
  .swiper {
    height: 250px;
  }
}

navigator {
  width: 100%;
  height: 100%;
}

.top {
  position: fixed;
  top: 0;
  display: flex;
  z-index: 9999 !important;
}

